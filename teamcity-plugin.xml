<?xml version="1.0" encoding="UTF-8"?>


<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
    <info>
        <name>deploy-runner</name>
        <display-name>Artifact Deployer</display-name>
        <version>@Version@</version>
        <description>Set of Runners for basic deployment tasks</description>
        <vendor>
            <name>JetBrains</name>
            <url>https://www.jetbrains.com</url>
        </vendor>
    </info>
    <deployment use-separate-classloader="true" node-responsibilities-aware="true"/>
  <dependencies>
    <plugin name="ssh-manager"/>
  </dependencies>
</teamcity-plugin>